.my-bar {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  z-index: 5001;
  background-color: white;
}

.nav-bar {
  display: flex;
  justify-content: space-between;
  align-items: center;
  width: 68%;
  padding-left: 15rpx;
  padding-right: 60rpx;
  background-color: rgb(255, 255, 255);
}

.nav-back {
  font-size: 25px;
}

.navigate-wraper {
  display: flex;
  align-items: center;
  height: 100%;
}

.navigate-item {
  display: flex;
  flex-direction: column;
  justify-content: flex-end;
  align-items: center;
  height: 100%;
  margin-right: 20rpx;
}

.navigate-title-show {
  font-size: 18px;
  color: #0EAB40;
}

.navigate-title-hidden {
  font-size: 17px;
}

.navigate-bar-show {
  width: 80%;
  height: 6rpx;
  margin-top: 10rpx;
  background-color: #0EAB40;
}

.navigate-bar-hidden {
  width: 80%;
  height: 6rpx;
  margin-top: 10rpx;
}