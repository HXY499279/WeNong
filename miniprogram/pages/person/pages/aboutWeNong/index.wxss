.wraper {
  width: 100%;
  min-height: 100vh;
  background-color: #F5F5F5;
}

.top {
  background-color: #0EAB40;
}

.top-logo {
  display: block;
  width: 300rpx;
  height: 300rpx;
  margin: 0 auto;
}

.header {
  display: flex;
  justify-content: space-between;
  align-items: center;
  width: 100%;
  height: 100rpx;
  padding: 0 40rpx;
  background-color: white;
  box-sizing: border-box;
}

.header-title {
  font-size: 18px;
  color: rgb(82, 81, 81);
}

.header-selector {
  color: #0EAB40;
}

.content {
  width: 100%;
  padding: 20rpx 40rpx;
  margin-top: 20rpx;
  color: rgb(82, 81, 81);
  background-color: white;
  box-sizing: border-box;
}

.content-title {
  display: block;
  margin-bottom: 10rpx;
  font-size: 18px;
}