.page {
  position: relative;
}

.background {
  width: 100%;
  height: 250rpx;
  background-color: rgb(99, 188, 81);
}

.wraper {
  width: 100%;
  position: absolute;
  top: 0;
}

.title {
  display: flex;
  justify-content: space-between;
  align-items: center;
  height: 100rpx;
  margin: 0 123rpx;
  font-size: 22px;
  letter-spacing: 5px;
}

.title-logo {
  width: 100rpx;
  height: 100rpx;
}

.table-wraper {
  width: 100%;
  padding: 10px 50rpx 0;
  background-color: white;
  border-radius: 20px !important;
  box-shadow: 0 -5px 5px rgb(0 33 79 / 11%) !important;
  box-sizing: border-box;
}

.item {
  display: flex;
  justify-content: space-between;
  align-items: center;
  height: 100rpx;
}

.item-name {
  height: 100%;
  line-height: 100%;
}

.item>.item-ipt {
  width: 60%;
  padding: 0 0 0 50rpx;
}

.item-images {
  height: auto;
  display: flex;
  align-items: flex-start;
}

.images {
  width: 60%;
  height: auto;
}

.submit-btn{
  margin: 150rpx 0;
  background-color: rgb(3, 154, 241);
}